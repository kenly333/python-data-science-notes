   ],
   "source": [
    "\n",
    "x = np.arange(1, 24)\n",
    "y = e(x)\n",
    "\n",
    "plt.xlabel('x')\n",
    "plt.ylabel('y')\n",
    "plt.plot(x,y,c='red')\n",
    "\n",
    "plt.axis([0,24,0,3])\n",
    "\n",
    "plt.show()"
   ]
